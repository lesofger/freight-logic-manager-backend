{
  "kind": "collectionType",
  "collectionName": "freight_rates",
  "info": {
    "singularName": "freight-rate",
    "pluralName": "freight-rates",
    "displayName": "Freight Rate"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "originPostalCode": {
      "type": "string",
      "required": true
    },
    "destinationPostalCode": {
      "type": "string",
      "required": true
    },
    "totalWeight": {
      "type": "decimal",
      "required": true,
      "description": "Total weight in grams"
    },
    "totalVolume": {
      "type": "decimal",
      "description": "Total volume in cubic millimeters"
    },
    "density": {
      "type": "decimal",
      "description": "Calculated density for freight class mapping"
    },
    "freightClass": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::freight-classe.freight-classe"
    },
    "items": {
      "type": "json",
      "description": "Line items with dimensions and quantity"
    },
    "applicableRates": {
      "type": "json",
      "description": "Available rates from price tables"
    },
    "selectedRate": {
      "type": "decimal",
      "description": "Selected rate in cents"
    },
    "distance": {
      "type": "integer",
      "description": "Distance in km (estimated)"
    },
    "status": {
      "type": "enumeration",
      "enum": ["pending", "calculated", "error"],
      "default": "pending"
    }
  }
}
